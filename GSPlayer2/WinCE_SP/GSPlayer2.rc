            MENUITEM "シャッフル",                       IDM_PLAY_SHUFFLE
        END
        POPUP "ツール"
        BEGIN
            MENUITEM "エフェクト...",                    IDM_TOOL_EFFECT
            MENUITEM SEPARATOR
            MENUITEM "音量を上げる",                      IDM_TOOL_VOLUP
            MENUITEM "音量を下げる",                      IDM_TOOL_VOLDOWN
            MENUITEM SEPARATOR
            MENUITEM "スリープタイマ...",                  IDM_TOOL_SLEEPTIMER
            MENUITEM "設定...",                       IDM_TOOL_OPTION
            MENUITEM SEPARATOR
            MENUITEM "バージョン情報...",                  IDM_HELP_ABOUT
            MENUITEM SEPARATOR
            MENUITEM "終了",                          IDM_APP_EXIT
        END
        MENUITEM SEPARATOR
        MENUITEM "画面オフ",                        IDM_TOOL_TOGGLEDISPLAY
    END
END

