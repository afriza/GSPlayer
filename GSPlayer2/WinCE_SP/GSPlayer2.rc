            MENUITEM "早送り",                         IDM_PLAY_FF
        END
        POPUP "モード"
        BEGIN
            MENUITEM "リピートなし",                      IDM_PLAY_REPEAT_NONE
            MENUITEM "全曲リピート",                      IDM_PLAY_REPEAT_ALL
